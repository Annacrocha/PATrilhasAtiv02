using System;

namespace Atividade_2_c_
{
	class Program
	{
		public static void Main(string[] args)
		{
			Console.WriteLine("Divisão entre dois números");
			Console.WriteLine("Insira o DIVIDENDO: ");
			int dividendo = Convert.ToInt32(Console.ReadLine());
			Console.WriteLine("Insira o DIVISOR:");
			int divisor = Convert.ToInt32(Console.ReadLine());
				
			int divisao = dividendo / divisor;
			
			Console.WriteLine("O resultado é: " + divisao);
			
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
	}
}
